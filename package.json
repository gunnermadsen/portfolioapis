    "@types/cookie-parser": "^1.4.1",
    "@types/cors": "^2.8.5",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.0",
    "@types/express-jwt": "0.0.42",
    "@types/http-errors": "^1.6.1",
    "@types/jest": "^24.0.18",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/md5": "^2.1.33",
