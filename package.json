  "version": "1.0.0",
  "description": "",
  "main": "start.js",
  "repository": "portfolioapis",
  "scripts": {
    "test:jest": "jest --watch --detectOpenHandles --runInBand",
    "build:webpack": "webpack",
    "build-ts": "rm -rf dist/* && tsc && npm run copy-repo && npm run make-directories && npm run copy-thumbnail-images",
    "postinstall": "npm run build-ts",
    "start": "npm run serve",
    "serve": "node dist/start.js",
    "watch-node": "nodemon dist/start.js",
    "debug:remote": "heroku ps:forward 9229 -a portfolioapis",
    "watch-ts": "tsc -w",
    "start-server": "ts-node ./start.ts",
    "dev": "cross-env TZ=UTC NODE_ENV=development tsnd --inspect --respawn start.ts",
    "copy-repo": "cp -r src/controllers/coolshare/repository dist/src/controllers/coolshare",
    "copy-repo2": "cp -r src/controllers/coolshare/repository dist/coolshare",
    "make-directories": "mkdir dist/uploads && mkdir dist/thumbnails",
    "copy-thumbnail-images": "cp -r thumbnails/* dist/thumbnails",
    "wp:build": "webpack --config webpack.config.js",
    "wp:start": "node --inspect dist/main.js"
  },
  "engines": {
    "node": "12.8.0",
    "npm": "6.11.3"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "dependencies": {
    "@decorators/di": "^1.0.2",
    "@decorators/socket": "^3.2.0",
    "@hasezoey/typegoose": "^5.9.2",
