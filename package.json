{
  "name": "portfolioapi",
  "author": "gunner",
  "license": "MIT",
  "version": "1.0.0",
  "description": "",
  "main": "start.js",
  "repository": "portfolioapis",
  "scripts": {
    "test:jest": "jest --watch --detectOpenHandles --runInBand",
    "serve": "cross-env TZ=UTC NODE_ENV=development tsnd --inspect --respawn start.ts",
    "build": "rm -rf dist/* && tsc && npm run make:folder-structure",
    "heroku-postbuild": "npm run build:prod",
    "start-server": "ts-node ./start.ts",
    "copy:repo:coolshare": "cp -r src/controllers/coolshare/repository dist/src/controllers/coolshare",
    "copy:repo:streamly": "cp -r src/controllers/streamly/repository dist/src/controllers/streamly",
    "copy:thumbnails": "cp -r thumbnails dist/thumbnails",
    "make:folder-structure": "npm run copy:repo:coolshare && npm run copy:repo:streamly && npm run make:directories && npm run copy:thumbnails",
    "make:directories": "mkdir dist/uploads && mkdir dist/src/controllers/coolshare/temp",
    "wp:build": "webpack --config webpack.dev.js && npm run make:directories",
    "wp:start": "npm run make-dir-structure && node dist/main"
  },
  "engines": {
    "node": "10.8.0",
    "npm": "6.13.4"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "_moduleAliases": {
    "@controllers": "src/controllers",
    "@models": "src/models",
    "@middleware": "src/middleware"
  },
  "dependencies": {
    "@overnightjs/core": "^1.6.9",
    "@overnightjs/logger": "^1.1.9",
    "@typegoose/typegoose": "^6.2.0",
    "@types/archiver": "^3.0.0",
    "@types/async": "^3.0.0",
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.17.0",
    "@types/compression": "0.0.36",
    "@types/cookie-parser": "^1.4.1",
    "@types/cors": "^2.8.5",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.0",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/mime": "^2.0.1",
    "@types/mongoose": "^5.5.6",
    "@types/multer": "^1.3.7",
    "@types/node": "^8.10.51",
    "@types/socket.io": "^2.1.4",
    "@types/uuid": "^3.4.5",
    "archiver": "^3.1.1",
    "async": "^3.0.1",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "crawler": "^1.2.1",
    "cross-env": "^6.0.3",
    "dotenv": "^8.0.0",
    "filepreview-es6": "^1.0.8",
    "helmet": "^3.21.2",
    "jsonwebtoken": "^8.5.1",
    "mime": "^2.4.4",
    "mongoose": "^5.8.3",
    "multer": "^1.4.1",
    "node-cmd": "^3.0.0",
    "pretty-file-icons": "^2.2.1",
    "socket-controllers": "0.0.5",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@types/fs-extra": "^8.0.0",
    "@types/jest": "^24.0.18",
    "@types/morgan": "^1.7.35",
    "@types/supertest": "^2.0.8",
    "clean-webpack-plugin": "^3.0.0",
    "fs-extra": "^8.1.0",
    "morgan": "^1.9.1",
    "supertest": "^4.0.2",
    "ts-jest": "^25.3.0",
    "ts-node": "^8.5.4",
    "ts-node-dev": "^1.0.0-pre.44",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.5.2",
    "webpack": "^4.41.5",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  }
}
